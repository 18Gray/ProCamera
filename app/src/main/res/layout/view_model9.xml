<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/rl_container_model9"
    android:layout_width="match_parent"
    android:layout_height="wrap_content" >

    <ImageView
        android:id="@+id/iv_icon_viewmodel9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        tools:src="@mipmap/saturation_black_24dp"
        />

    <TextView
        android:id="@+id/tv_title_viewmodel9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@id/iv_icon_viewmodel9"
        tools:text="调整"
        />

    <TextView
        android:id="@+id/tv_content_viewmodel9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@id/iv_icon_viewmodel9"
        android:layout_below="@id/tv_title_viewmodel9"
        tools:text="调整"
        />

</RelativeLayout>